package co.zerono.pc.init;

import co.zerono.pc.blocks.BlockPC;
import co.zerono.pc.blocks.BlockPoorOre;
import co.zerono.pc.reference.Reference;
import cpw.mods.fml.common.registry.GameRegistry;

@GameRegistry.ObjectHolder(Reference.MOD_ID)
public class ModBlocks 
{
	public static final BlockPC ore = new BlockPoorOre();
	
	public static void init()
	{
		initBlocks();
	}
	
	public static void initBlocks()
	{
		GameRegistry.registerBlock(ore, "ore");
	}
}

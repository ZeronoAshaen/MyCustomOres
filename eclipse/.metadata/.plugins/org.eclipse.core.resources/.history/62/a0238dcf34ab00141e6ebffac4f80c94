package co.zerono.pc.handlers.texture;

import co.zerono.pc.blocks.BlockOre;
import co.zerono.pc.reference.Reference;
import net.minecraft.client.renderer.texture.TextureAtlasSprite;

public class GetTexture extends TextureAtlasSprite 
{
	private BlockOre blockOre;
	protected GetTexture(BlockOre blockOre) 
	{
		super(Reference.MOD_ID.toLowerCase() + ":ore" + blockOre.getType() + blockOre.getName());
		this.blockOre = blockOre;
	}

}
